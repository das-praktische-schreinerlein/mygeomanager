{
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": true,
    "diagnostics": false,
    "traceResolution": false,
    "listFiles": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strictFunctionTypes": false,
    "noStrictGenericChecks": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../../dist/tsc-out/backend",
    "module": "es2015",
    "baseUrl": "./",
    "types": ["node"],
    "paths": {
      "*": [ "../../node_modules/@types/*", "*"]
    }
  },
  "exclude": [
    "node_modules",
    "test.ts",
    "**/*.spec.ts",
    "dist"
  ]
}
